   "source": [
    "def convertToTitle(columnNumber:int) -> str:\n",
    "    columnNumber-=1\n",
    "    dmod = divmod(columnNumber, 26)\n",
    "    if dmod[0]>0:\n",
    "        return convertToTitle(columnNumber=dmod[0])+chr(dmod[1]+65)\n",
    "    else:\n",
    "        return chr(columnNumber+65)"
   ]
