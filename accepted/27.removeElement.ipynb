   "source": [
    "def removeElement(nums: list, val: int) -> int:\n",
    "    k=0\n",
    "    for i in range(len(nums)):\n",
    "        if nums[k] == val:\n",
    "            nums.pop(k)\n",
    "        else:\n",
    "            k+=1\n",
    "    return k, nums"
   ]
