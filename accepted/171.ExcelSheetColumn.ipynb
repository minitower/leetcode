   "source": [
    "def titleToNumber(columnTitle: str) -> int:\n",
    "    answer=0\n",
    "    for idx, char in enumerate(columnTitle[::-1]):\n",
    "        title_num = ord(char)-64\n",
    "        if idx==0:\n",
    "            answer+=title_num\n",
    "        else:\n",
    "            answer+=26**idx*title_num\n",
    "        print(answer)\n",
    "    return answer"
   ]
