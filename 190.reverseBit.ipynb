   "source": [
    "def reverseBit(n:int) -> int:\n",
    "    num_bin = bin(n)[2:]\n",
    "    if len(num_bin)<32:\n",
    "        num_bin=''.join(['0']*(32-len(num_bin)))+num_bin\n",
    "    return int(num_bin[::-1], 2)"
   ]
